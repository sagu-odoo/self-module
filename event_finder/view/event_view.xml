<?xml version="1.0"?>
<odoo>
    <record id="event_tree" model="ir.ui.view">
            <field name="name">event.property.tree</field>
            <field name="model">event.property</field>
            <field name="arch" type="xml">
            
              <tree string="estate_property">
                <field name="name"/>
                <field name="date"/>
                <field name="place"/>
                <field name="description"/>
             </tree>
            </field>
    </record>

   <record id="event_finder_action" model="ir.actions.act_window">
        <field name="name">On Going Event</field>
        <field name="res_model">event.property</field>
        <field name="view_mode">tree,form</field>
   </record>
   <record id="event_finder_action2" model="ir.actions.act_window">
        <field name="name">Organiser</field>
        <field name="res_model">event.property</field>
        <field name="view_mode">tree,form</field>
   </record>

</odoo>